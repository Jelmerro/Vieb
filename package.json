{
    "name": "vieb",
    "version": "12.7.0",
    "description": "Vim Inspired Electron Browser",
    "keywords": [
        "Vim",
        "Electron",
        "Browser",
        "Internet"
    ],
    "homepage": "https://vieb.dev",
    "bugs": "https://github.com/Jelmerro/Vieb/issues/",
    "repository": {
        "url": "https://github.com/Jelmerro/Vieb",
        "type": "git"
    },
    "funding": "https://github.com/sponsors/Jelmerro/",
    "license": "GPL-3.0-or-later",
    "author": "Jelmer van Arnhem",
    "type": "commonjs",
    "exports": "./app/*",
    "main": "./app/index.js",
    "scripts": {
        "dev": "electron app --datafolder=./ViebData/ --config-order=datafolder-only",
        "eslint": "eslint",
        "fix": "eslint --fix",
        "lint": "npm run eslint && npm run tsc",
        "start": "electron app",
        "test": "node --test",
        "test:all": "npm run test && npm run lint && echo 'All good :)'",
        "tsc": "tsc",
        "update": "npx -y jelmerro/nus"
    },
    "dependencies": {
        "@ghostery/adblocker-electron": "2.14.1",
        "@ghostery/adblocker-electron-preload": "2.14.1",
        "@mozilla/readability": "0.6.0",
        "darkreader": "4.9.96",
        "highlight.js": "11.11.1",
        "linkedom": "0.18.12",
        "marked": "15.0.12",
        "marked-base-url": "1.1.8",
        "marked-highlight": "2.2.3",
        "picomatch": "4.0.3"
    },
    "devDependencies": {
        "@electron/asar": "4.0.1",
        "@types/picomatch": "4.0.2",
        "electron": "40.6.1",
        "electron-builder": "26.8.1",
        "eslint-config": "github:Jelmerro/eslint-config",
        "glob": "13.0.6",
        "terser-webpack-plugin": "5.3.16",
        "type-config": "github:Jelmerro/type-config",
        "webpack": "5.105.3",
        "webpack-node-externals": "3.0.0"
    },
    "engines": {
        "node": ">=22"
    }
}
